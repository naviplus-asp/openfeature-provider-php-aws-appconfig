{
    "name": "openfeature/php-aws-appconfig-provider",
    "description": "OpenFeature provider for PHP that uses AWS AppConfig as the backend configuration store",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "openfeature",
        "feature-flags",
        "aws",
        "appconfig",
        "php"
    ],
    "authors": [
        {
            "name": "OpenFeature Contributors",
            "homepage": "https://openfeature.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "open-feature/sdk": "^1.0",
        "aws/aws-sdk-php": "^3.0",
        "psr/log": "^1.1",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "php-coveralls/php-coveralls": "^2.5",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenFeature\\Providers\\AwsAppConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenFeature\\Providers\\AwsAppConfig\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-integration": "phpunit --testsuite=Integration",
        "test-unit": "phpunit --testsuite=Unit",
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs --standard=PSR12 src/ tests/",
        "phpcbf": "phpcbf --standard=PSR12 src/ tests/",
        "check": [
            "@phpstan",
            "@phpcs",
            "@test"
        ],
        "agent:start": "./scripts/start-agent.sh",
        "agent:stop": "./scripts/stop-agent.sh",
        "test:with-agent": [
            "@agent:start",
            "@test-integration",
            "@agent:stop"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
